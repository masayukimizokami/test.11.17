<h1>プロフィールページ</h1>

<%= form_for @user do |f| %>

<div class="field">
    
  <div class="field">
    <%= f.label :アイコン画像 %><br />
    <%= f.text_field :name, autofocus: true, autocomplete: "ファイルを選択" %>
  </div>

　<div class="field">　
    <%= f.label :名前 %><br />
    <%= f.text_field :name, autofocus: true, autocomplete: "名前" %>
  </div>

  <div class="field">
    <%= f.label :自己紹介 %><br />
    <%= f.text_area :profile, autofocus: true, autocomplete: "自己紹介を入力" %>
  </div>


   <div class="actions">
    <%= f.submit "更新" %>
  </div>

<% end %>

