<h1>検索結果</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Height</th>
      <th>Weight</th>
      <th>Gender</th>
      <th colspan="3"></th>
    </tr>
  </thead>

    <tbody>
    <div class="search_box">
      <% @results.each do |room| %>
      
        <tr>
          <td><%= room.room_name %></td>
          <td><%= room.room_price %></td>
           <td><%= link_to 'Show', room %></td>
        </tr>
      <% end %>

    </tbody>
  </div>
</table>

<%= link_to 'Top', rooms_path %>

